  "bugs": {
    "url": ""
  },
  "homepage": "",
  "lint-staged": {
    "**/*.js": [
      "npm run lint",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-publish": "npm run build"
    }
  },
